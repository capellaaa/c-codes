#include <stdio.h>
#include <stdlib.h>

int main()

{
    int num1, num2,  num3 ;

    printf ("number 1:") ;
    scanf ("%d",&num1) ;
    printf ("number 2:") ;
    scanf ("%d",&num2) ;
    printf ("number 3:") ;
    scanf ("%d",&num3) ;

    if (num1> num2 && num1>num3)
    {
        printf ("maximum number : %d", num1) ;
    }
    else if (num2>num3)
    {
        printf ("maximum number : %d", num2) ;
    }

    else
    {
        printf ("maximum number : %d", num3) ;
    }

    return 0;
}
